<robot name="km_dolly">
  <material name="orange">
    <color rgba="1.0 0.5 0.2 1" />
  </material>
  <material name="gray">
    <color rgba="0.7 0.7 0.7 1" />
  </material>
  <material name="red">
    <color rgba="0.4 0 0 1" />
  </material>
  <link name="imu_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.01 0.02 0.02" />
      </geometry>
      <material name="orange" />
    </visual>
  </link>

  <!--<link name="horizontal_laser_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.02" radius="0.03" />
      </geometry>
      <material name="red" />
    </visual>
  </link>
  -->

  <link name="base_link">
    <visual>
      <geometry>
        <mesh filename="package://km_dolly/stl/Keigan_Dolly_frame_resized.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <material name="gray" />
    </visual>
  </link>

  <joint name="imu_link_joint" type="fixed">
    <parent link="base_link" />
    <child link="imu_link" />
    <origin xyz="0.10 0 0.062" />
  </joint>

  <!--
  <joint name="horizontal_laser_link_joint" type="fixed">
    <parent link="base_link" />
    <child link="horizontal_laser_link" />
    <origin xyz="0 0 0.40" rpy="0 0 0"/>
  </joint>
  -->
  <link name="right_wheel">
    <visual>
      <geometry>
        <mesh filename="package://km_dolly/stl/Keigan_Dolly_Wheel_R_resized.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 3.1416"/>
      <material name="gray" />
    </visual>
  </link>

  <joint name="right_wheel_joint" type="continuous">
    <parent link="right_km1" />
    <child link="right_wheel" />
    <origin xyz="0.03 0 0" rpy="0 0 0"/>
    <axis xyz="-1 0 0"/>
  </joint>
  <joint name="right_km1_joint" type="fixed">
    <parent link="base_link" />
    <child link="right_km1" />
    <origin xyz="0.09 0 0.062" rpy="0 0 0"/>
  </joint>

  <link name="right_km1">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://km_dolly/stl/Keigan_Dolly_KM-1_L_resized.stl" />
      </geometry>
      <material name="gray" />
    </visual>
  </link>

  <link name="left_wheel">
    <visual>
      <geometry>
        <mesh filename="package://km_dolly/stl/Keigan_Dolly_Wheel_L_resized.stl" />
      </geometry>
        <origin xyz="0 0 0" rpy="0 0 3.1416"/>
      <material name="gray" />
    </visual>
  </link>

  <joint name="left_wheel_joint" type="continuous">
    <parent link="left_km1" />
    <child link="left_wheel" />
    <origin xyz="-0.03 0 0" rpy="0 0 0"/>
    <axis xyz="-1 0 0"/>
  </joint>

  <joint name="left_km1_joint" type="fixed">
    <parent link="base_link" />
    <child link="left_km1" />
    <origin xyz="-0.09 0 0.062" rpy="0 0 0" />
  </joint>

  <link name="left_km1">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://km_dolly/stl/Keigan_Dolly_KM-1_R_resized.stl" />
      </geometry>
      <material name="gray" />
    </visual>
  </link>


</robot>
